#scope : std
    #scope : reg
        #alias : stackPointer : $sp
        #alias : framePointer : $fp
        #alias : loopCounter : $lc
        #alias : accumulator : $ac
        #alias : codePointer : $cp
        #alias : exitCode : $ec
    #end / reg

    / Print a string to the console
    #funx : prints
    / Print value of specified type to the console
    #funx : printt
    / Sleep n milliseconds
    #funx : sleepms
#end / std