#reqmod : "std"

pushc.addr : "Hello world!"
#static : destBuff : 13
pushc.addr : destBuff

#alias : src : ~0
#alias : dest : ~8

#label : copyLoopBegin
	drf : std>>reg>>accumulator : @src
    mov.u8 : @dest : @$ac
    add.addr : src : 1
    add.addr : dest : 1
jne.u8 : copyLoopBegin : @std>>reg>>accumulator : 0

exit