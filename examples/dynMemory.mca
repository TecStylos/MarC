#reqmod : "std"
#manperm : >>std>>ext>>prints

        / Memory init
#alias : BUFFSIZE : 14
#alias : src : "Hello world!\n"
#static : dst : ^addr
alloc : dst : BUFFSIZE

call : >>std>>copyString : addr.@dst : addr.src

calx : >>std>>ext>>prints : addr."src: "
calx : >>std>>ext>>prints : addr.src
calx : >>std>>ext>>prints : addr."dst: "
calx : >>std>>ext>>prints : addr.@dst

free : @dst