#reqmod : "std"
#manperm : >>stdext>>prints
#manperm : >>stdext>>scant

#static : var : ^i64

prints : "Var: "
scant.i64 : var

switch.i64 : @var
    case : 1
        println : "One\n"
        fallthrough
    case : 2
        println : "Two\n"
    case : 3
        println : "Three\n"
        fallthrough
    case : 4
        println : "Four\n"
        fallthrough
    case : 5
        println : "Five\n"
    default
        println : "Default\n"
endswitch